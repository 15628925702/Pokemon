                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                  <VBox prefHeight="36.0" prefWidth="175.0" />
                  <Button mnemonicParsing="false" onMouseClicked="#onExitClicked" prefHeight="63.0" prefWidth="168.0" styleClass="exit-button" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
